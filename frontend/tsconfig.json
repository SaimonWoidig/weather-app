{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "astro/client",
      "vite/client",
      "vitest/globals"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  },
  "exclude": [
    "dist"
  ]
}